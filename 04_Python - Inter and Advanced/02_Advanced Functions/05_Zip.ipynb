   "source": [
    "# Zip\n",
    "The zip function in Python is used to combine elements from multiple iterables (e.g., lists, tuples) into tuples. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "<zip object at 0x7fd3ce787080>\n",
      "[(1, 'a'), (2, 'b'), (3, 'c')]\n"
     ]
    }
   ],
   "source": [
    "list1 = [1, 2, 3]\n",
    "list2 = ['a', 'b', 'c', 'd'] \n",
    "\n",
    "zipped = zip(list1, list2) # Zipped to the length of the shorter iterable\n",
    "print(zipped)\n",
    "print(list(zipped))\n"
   ]
  }
